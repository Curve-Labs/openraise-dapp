{
    "name": "BC-DAPP",
    "version": "0.2.2",
    "description": "Decentralized app for fundraising in DXdao.",
    "scripts": {
        "start": "./scripts/start.sh",
        "dev": "./scripts/dev.sh",
        "deploy": "node scripts/deploy.js",
        "deploy-contracts": "node scripts/deployContracts.js",
        "build": "node scripts/build.js",
        "test": "node scripts/test.js",
        "loadDeployments": "node scripts/loadDeployments.js"
    },
    "engines": {
        "node": "10.16.3"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/levelkdev/BC-DAPP.git"
    },
    "bugs": {
        "url": "https://github.com/levelkdev/BC-DAPP/issues"
    },
    "homepage": "./",
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "jest": {
        "roots": [
            "<rootDir>/src"
        ],
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/**/*.d.ts"
        ],
        "setupFiles": [
            "react-app-polyfill/jsdom"
        ],
        "setupFilesAfterEnv": [],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
            "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
        ],
        "testEnvironment": "jest-environment-jsdom-fourteen",
        "transform": {
            "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
            "^.+\\.module\\.(css|sass|scss)$"
        ],
        "modulePaths": [],
        "moduleNameMapper": {
            "^react-native$": "react-native-web",
            "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "web.ts",
            "ts",
            "web.tsx",
            "tsx",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ],
        "watchPlugins": [
            "jest-watch-typeahead/filename",
            "jest-watch-typeahead/testname"
        ]
    },
    "babel": {
        "presets": [
            "react-app"
        ],
        "plugins": [
            [
                "@babel/plugin-proposal-decorators",
                {
                    "legacy": true
                }
            ]
        ]
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx}": [
            "tslint --project tsconfig.json -c tslint.json --fix",
            "prettier --write",
            "git add"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "dependencies": {
        "@babel/core": "7.10.2",
        "@babel/plugin-proposal-decorators": "^7.6.0",
        "@babel/plugin-transform-react-jsx": "^7.3.0",
        "@ledgerhq/hw-app-eth": "^5.16.0",
        "@ledgerhq/hw-transport-u2f": "^5.15.0",
        "@openzeppelin/contracts-ethereum-package": "^2.5.0",
        "@reach/dialog": "^0.10.3",
        "@svgr/webpack": "5.4.0",
        "@types/jest": "^26.0.0",
        "@types/node": "^14.0.13",
        "@types/react": "^16.9.19",
        "@types/react-dom": "^16.9.5",
        "@typescript-eslint/eslint-plugin": "^3.2.0",
        "@typescript-eslint/parser": "^3.2.0",
        "@web3-react/core": "^6.0.7",
        "@web3-react/injected-connector": "^6.0.7",
        "@web3-react/network-connector": "^6.0.7",
        "@web3-react/walletconnect-connector": "^6.0.9",
        "@web3-react/walletlink-connector": "^6.0.9",
        "abi-decoder": "^2.2.2",
        "babel-eslint": "10.1.0",
        "babel-jest": "^26.0.1",
        "babel-loader": "8.1.0",
        "babel-plugin-named-asset-import": "^0.3.4",
        "babel-preset-react-app": "^9.0.2",
        "bignumber.js": "^9.0.0",
        "camelcase": "^6.0.0",
        "case-sensitive-paths-webpack-plugin": "2.3.0",
        "chart.js": "^2.9.3",
        "copy-to-clipboard": "^3.3.1",
        "css-loader": "3.5.3",
        "dotenv": "^8.1.0",
        "dotenv-expand": "5.1.0",
        "ethereumjs-tx": "^2.1.1",
        "ethereumjs-util": "^7.0.2",
        "ethers": "^4.0.45",
        "file-loader": "6.0.0",
        "hdkey": "^2.0.1",
        "html-webpack-plugin": "4.3.0",
        "identity-obj-proxy": "3.0.0",
        "is-wsl": "^2.2.0",
        "isomorphic-fetch": "^2.2.1",
        "jazzicon": "^1.5.0",
        "loglevel": "^1.6.4",
        "mini-css-extract-plugin": "0.9.0",
        "mobx": "^5.14.0",
        "mobx-react": "^6.1.3",
